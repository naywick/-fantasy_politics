<h1>Here are your results</h1>
<div class="container">
<% @results.each do |type| %>
  <h3><%= type["index"] %></h3>
  <% type["hits"].each do |instance| %>
    <% if type["index"] == 'User' %>
      <p><%= instance["username"] %></p>
    <% elsif type["index"] == 'Politician' %>
      <p><%= instance["first_name"] %> <%= instance["last_name"] %></p>
    <% elsif type["index"] == 'PoliticalParty' %>
      <p><%= instance["name"]%></p>
    <% elsif type["index"] == 'League' %>
      <p><%= instance["name"]%></p>
    <% end %>
    <% end %>
  <% end %>
</div>
